<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>价格系统</title>
       <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="../media/layui/css/layui.css">
    <script type="application/javascript" src="../media/js/jquery.min.js"></script>
    <script type="application/javascript" src="../media/layui/layui.js"></script>
    <script>
        function submit() {
            var num1_min = $("#num1_min").val();
            var num1_max = $("#num1_max").val();
            var price1 = $("#price1").val();

            var num2_min = $("#num2_min").val();
            var num2_max = $("#num2_max").val();
            var price1 = $("#price2").val();

            var num3_min = $("#num3_min").val();
            var num3_max = $("#num3_max").val();
            var price3 = $("#price3").val();

            var num4_min = $("#num4_min").val();
            var num4_max = $("#num4_max").val();
            var price4 = $("#price4").val();



        }
